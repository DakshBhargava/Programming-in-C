/* Perform diagonal traversal of a matrix */

#include <stdio.h>
int main() {
    int m, n, i, j, start;
    scanf("%d %d", &m, &n);

    int a[m][n];

    for(i = 0; i < m; i++)
        for(j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    for(start = 0; start < n; start++) {
        i = 0;
        j = start;
        while(i < m && j >= 0) {
            printf("%d ", a[i][j]);
            i++;
            j--;
        }
    }

    for(start = 1; start < m; start++) {
        i = start;
        j = n - 1;
        while(i < m && j >= 0) {
            printf("%d ", a[i][j]);
            i++;
            j--;
        }
    }

    return 0;
}
