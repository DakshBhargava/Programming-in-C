/* Check if a string is a palindrome */

#include <stdio.h>
int main() {
    char s[200];
    int i = 0, len = 0, flag = 1;

    fgets(s, 200, stdin);

    while(s[len] != '\0')
        len++;

    if(len > 0 && s[len - 1] == '\n')
        len--;

    for(i = 0; i < len / 2; i++) {
        if(s[i] != s[len - 1 - i]) {
            flag = 0;
            break;
        }
    }

    if(flag)
        printf("Palindrome");
    else
        printf("Not Palindrome");

    return 0;
}
