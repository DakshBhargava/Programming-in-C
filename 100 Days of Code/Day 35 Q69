#include <stdio.h>

int main() {
    int a[100], n;
    int max1, max2;

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter %d elements:\n", n);
    for(int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    max1 = max2 = -2147483648;

    for(int i = 0; i < n; i++) {
        if(a[i] > max1) {
            max2 = max1;
            max1 = a[i];
        }
        else if(a[i] > max2 && a[i] < max1) {
            max2 = a[i];
        }
    }

    if(max2 == -2147483648)
        printf("No second largest element.\n");
    else
        printf("Second largest element = %d\n", max2);

    return 0;
}
