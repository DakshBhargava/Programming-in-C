/* Count characters in a string without using built-in length functions */

#include <stdio.h>
int main() {
    char s[100];
    int i = 0, count = 0;

    fgets(s, 100, stdin);

    while(s[i] != '\0') {
        count++;
        i++;
    }

    if(count > 0 && s[count - 1] == '\n')
        count--;

    printf("%d", count);

    return 0;
}
