/* Reverse a string */

#include <stdio.h>
int main() {
    char s[200];
    int i = 0, len = 0;

    fgets(s, 200, stdin);

    while(s[len] != '\0')
        len++;

    if(len > 0 && s[len - 1] == '\n')
        len--;

    for(i = len - 1; i >= 0; i--)
        printf("%c", s[i]);

    return 0;
}
