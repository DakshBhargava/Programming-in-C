/* Count vowels and consonants in a string */

#include <stdio.h>
int main() {
    char s[200];
    int i = 0, v = 0, c = 0;

    fgets(s, 200, stdin);

    while(s[i] != '\0') {
        char ch = s[i];
        if(ch >= 'A' && ch <= 'Z') ch += 32;

        if(ch >= 'a' && ch <= 'z') {
            if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u')
                v++;
            else
                c++;
        }
        i++;
    }

    printf("%d %d", v, c);

    return 0;
}
