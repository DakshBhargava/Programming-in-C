/* Find the sum of each row of a matrix and store it in an array */

#include <stdio.h>
int main() {
    int m, n, i, j;
    scanf("%d %d", &m, &n);
    int a[m][n], rowSum[m];

    for(i = 0; i < m; i++)
        for(j = 0; j < n; j++)
            scanf("%d", &a[i][j]);

    for(i = 0; i < m; i++) {
        rowSum[i] = 0;
        for(j = 0; j < n; j++)
            rowSum[i] += a[i][j];
    }

    for(i = 0; i < m; i++)
        printf("%d ", rowSum[i]);

    return 0;
}
